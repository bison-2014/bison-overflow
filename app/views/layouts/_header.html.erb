<header>
	<nav>
		<ul>
				<li><a href="/">Home</a></li>
			<% if logged_in? %>
				<li><%= link_to "Log Out", logout_path, method: "delete" %></li>
				<li><%= link_to "My User Profile", user_path(current_user) %> </li>
			<% else %>
				<li><%= render 'users/login' %></li>
			<% end %>
		</ul>
	</nav>
</header>
